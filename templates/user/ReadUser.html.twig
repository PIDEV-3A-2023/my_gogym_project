<h1> Liste des utilisateurs </h1>
<table border="1">
    <tr>
    <th>ID</th>
    <th>Nom</th>
    <th>Prenom</th>
    <th>Username</th>
    <th>Email</th>
    <th>Password</th>
    <th>Editer</th>
    </tr>
    {% for u in Users %}
    <tr>
        <td>
            {{u.getId()}}
        </td>
        <td>
            {{u.getNom()}}
        </td>
        <td>
            {{u.getPrenom()}}
        </td>
        <td>
            {{u.getUsername()}}
        </td>
        <td>
            {{u.getEmail()}}
        </td>
        <td>
            {{u.getPassword()}}
        </td>
        <td>
            <a href="{{path('Delete_User',{'id':u.id})}}">Remove</a>
            <a href="{{path('Update_User',{'id':u.id})}}">Update</a>
        </td>
    </tr>
    {% endfor %}
</table>
<br>
<a href="{{path('Create_User')}}">Create Utilisateur </a>
